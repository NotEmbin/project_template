{
	"type":"object",
	"parameters":{
		"text":{
			"type":"string",
			"incompatible_parameters":["translate"]
		},
		"translate":{
			"type":"string",
			"incompatible_parameters":["text"]
		},
		"color":{
			"type":"color",
			"default_color":"#ffffff"
		},
		"outline_color":{
			"type":"color",
			"default_color":"#ffffff"
		},
		"outline_width":{
			"type":"number",
			"default_number":0
		},
		"translation_data":{
			"type":"any",
			"require_parameters":["translate"]
		},
		"replacers":{
			"type":"array",
			"array_type":"parser_reference",
			"parser_reference":"engine:text_replacer",
			"array_length":"any"
		},
		"letter_replacer":{
			"type":"letter_replacer"
		},
		"extra":{
			"type":"parser_reference",
			"parser_reference":"engine:text"
		}
	}
}